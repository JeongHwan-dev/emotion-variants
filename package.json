{
  "name": "emotion-variants",
  "version": "0.1.0",
  "description": "Type-safe variant API for Emotion",
  "keywords": [
    "emotion",
    "variants",
    "css-in-js",
    "typescript",
    "design-system"
  ],
  "author": "Jeonghwan Park",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JeongHwan-dev/emotion-variants"
  },
  "homepage": "https://github.com/JeongHwan-dev/emotion-variants#readme",
  "bugs": {
    "url": "https://github.com/JeongHwan-dev/emotion-variants/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:types": "tsc --noEmit",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "prepublishOnly": "pnpm check:fix && pnpm build && pnpm test",
    "generate:labels": "node .github/scripts/generate-labels.mjs",
    "commit": "cz",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,mjs,ts,tsx,mts}": [
      "pnpm check:fix"
    ],
    "*.{json,css,md}": [
      "pnpm format:fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./commitizen.config.mjs"
    }
  },
  "peerDependencies": {
    "@emotion/react": "^11.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@emotion/serialize": "^1.1.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@emotion/react": "^11.13.5",
    "@testing-library/react": "^14.3.1",
    "@types/node": "^20.17.9",
    "@types/react": "^18.3.12",
    "commitizen": "^4.3.1",
    "happy-dom": "^15.11.7",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^1.6.0"
  },
  "packageManager": "pnpm@9.0.0"
}
